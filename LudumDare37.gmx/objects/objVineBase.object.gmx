else if tileX==objPlanter.w-1 dx = 1
else if tileY==0 dy = -1
else if tileY==objPlanter.h-1 dy = 1

bulbObj = instance_create(x+global.tileSize*dx, y+global.tileSize*dy, objBulb)
bulbObj.parent = id
//bulbObj.visible=0

segmentList = ds_list_create()

var seg = instance_create(x, y, objVineSegment);
ds_list_add(segmentList, seg)
if dx == -1 { seg.sprite_index=sprVineStartL bulbObj.image_angle=180 facing = 'left'}
else if dx == 1 { seg.sprite_index=sprVineStartR bulbObj.image_angle=0 facing = 'right'}
else if dy == -1 { seg.sprite_index=sprVineStartU bulbObj.image_angle=90 facing = 'top'}
else if dy == 1 { seg.sprite_index=sprVineStartD bulbObj.image_angle=270 facing = 'bottom'}

lastDx = dx
lastDy = dy

doneGrowing = false
maxLength = 12
destroyVisible = false

destroying = false
destroyT = 0
spikeIndex = 0
destroyState = 1
</string>
